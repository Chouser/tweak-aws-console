{
  "manifest_version": 2,
  "name": "Tweak AWS Console by Chouser",
  "version": "1.0",
  "permissions": [
    "activeTab",
    "storage",
    "https://signin.aws.amazon.com/*"
  ],
  "content_scripts": [
    {
      "matches": ["https://signin.aws.amazon.com/*"],
      "css": ["styles.css"]
    }
  ],
  "browser_action": {
    "default_icon": {
        /*
      "16": "images/icon16.png",
      "48": "images/icon48.png",
      "128": "images/icon128.png"
      */
    }
  },
  "icons": {
      /*
    "16": "images/icon16.png",
    "48": "images/icon48.png",
    "128": "images/icon128.png"
    */
  },
  "permissions": ["activeTab"]
}
